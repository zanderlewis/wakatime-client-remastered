{
  "name": "wakatime-client-remastered",
  "version": "1.0.3",
  "description": "A NodeJS client for the WakaTime API",
  "license": "MIT",
  "keywords": [
    "wakatime",
    "wakatime client"
  ],
  "homepage": "https://github.com/zanderlewis/wakatime-client-remastered/#readme",
  "readme": "https://github.com/zanderlewis/wakatime-client-remastered/#readme",
  "bugs": {
    "url": "https://github.com/zanderlewis/wakatime-client-remastered/issues"
  },
  "author": {
    "name": "zander@zanderlewis.dev"
  },
  "scripts": {
    "build": "BABEL_ENV=production npx rollup -c --bundleConfigAsCjs",
    "codecov": "npx codecov",
    "commitmsg": "npx commitlint -E HUSKY_GIT_PARAMS",
    "deploy": "npm run semantic-release",
    "gc": "commit",
    "is-build-es5": "npx es-check es5 './build/**/*.!(esm).js'",
    "lint": "npx eslint --ext .js .",
    "prepare": "npm run build",
    "semantic-release": "npx semantic-release"
  },
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {},
  "peerDependencies": {
    "axios": ">=0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.0",
    "@babel/plugin-transform-object-rest-spread": "^7.22.5",
    "@babel/plugin-transform-regenerator": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/runtime": "^7.22.6",
    "@commitlint/cli": "^17.7.3",
    "@commitlint/config-angular": "^19.5.0",
    "@commitlint/prompt": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@eslint/object-schema": "^2.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "dotenv": "^16.3.1",
    "es-check": "^7.0.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^9.1.6",
    "lru-cache": "^11.0.1",
    "marked": "^5.1.1",
    "rollup": "^3.29.4",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "semantic-release": "^24.1.2"
  }
}